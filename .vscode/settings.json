{
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "editor.indentSize": 2,
  "editor.wordWrap": "on",
  "vite.autoStart": true,
  "vite.devCommand": "pnpm run dev",
  "editor.formatOnSave": true,
  "tailwindCSS.classAttributes": ["class", "className", "ngClass", "class:list", "inputClassName", "iconClassName"],
  "todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text-and-comment",
    "foreground": "black",
    "background": "white",
    "opacity": 50,
    "iconColour": "blue",
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "foreground": "orange",
      "background": "transparent",
      "iconColour": "white"
    },
    "NOTE": {
      "icon": "note",
      "foreground": "white",
      "background": "blue",
      "iconColour": "gray"
    },
    "COMMENT": {
      "icon": "note",
      "foreground": "white",
      "background": "gray",
      "iconColour": "gray"
    },
    "FIXME": {
      "foreground": "black",
      "background": "yellow",
      "iconColour": "yellow"
    },
    "BUG": {
      "foreground": "black",
      "background": "red",
      "iconColour": "red"
    },
    "[ ]": {
      "icon": "check",
      "foreground": "black",
      "background": "yellow",
      "iconColour": "yellow"
    },
    "[x]": {
      "icon": "check",
      "foreground": "white",
      "background": "green",
      "iconColour": "green"
    }
  },
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "NOTE", "COMMENT", "[ ]", "[x]"],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*"
}
