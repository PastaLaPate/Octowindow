{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "printWidth": 80,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULE>",
    "",
    "@/lib/(.*)$",
    "@/components/(.*)$",
    "",
    "@/(.*)$",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
